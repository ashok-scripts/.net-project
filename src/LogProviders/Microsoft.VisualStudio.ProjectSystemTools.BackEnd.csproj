<Project Sdk="Microsoft.NET.Sdk">
  
  <Import Project="..\..\build\import\VisualStudio.props" />
  
  <PropertyGroup>
    <AssemblyName>Microsoft.VisualStudio.ProjectSystem.Tools.Providers</AssemblyName>
    <RootNamespace>Microsoft.VisualStudio.ProjectSystem.Tools.Providers</RootNamespace>
    
    <!-- VSIX -->
    <GeneratePkgDefFile>true</GeneratePkgDefFile>
    <CreateVsixContainer>false</CreateVsixContainer>
    
    <IsProductComponent>false</IsProductComponent>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <LangVersion>latest</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\LogModel\Microsoft.VisualStudio.ProjectSystemTools.Shared.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="RoslynLogging\RoslynLoggingResources.Designer.cs">
      <DependentUpon>RoslynLoggingResources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="BuildLoggingResources.resx">
      <GenerateSource>true</GenerateSource>
      <SubType>Designer</SubType>
      <Generator>MSBuild:_GenerateResxSource</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="RoslynLogging\RoslynLoggingResources.resx">
      <SubType>Designer</SubType>
      <LastGenOutput>RoslynLoggingResources.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>
</Project>
