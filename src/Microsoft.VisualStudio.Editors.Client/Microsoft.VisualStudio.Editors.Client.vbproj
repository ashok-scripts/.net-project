<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\build\import\VisualStudioDesigner.props" />
  <PropertyGroup>
    <Prefer32Bit>false</Prefer32Bit>
    <StrongNameKeyId>Microsoft</StrongNameKeyId>
    <TargetFramework>net472</TargetFramework>
    <IsVsixProject>true</IsVsixProject>
    <CreateVsixContainer>false</CreateVsixContainer>
    <GeneratePkgDefFile>true</GeneratePkgDefFile>
    <!-- TODO: Function/Property doesn't return a value on all code paths (https://github.com/dotnet/project-system/issues/2592) -->
    <NoWarn>$(NoWarn);42353;42355;42105;NU5125</NoWarn>

    <!-- Nuget -->
    <IsPackable>true</IsPackable>
    <Description>Implementation of some designers and project property page editors in VisualStudio Client.</Description>
    <Summary>Editors implementation for Project Properties in the Client</Summary>
    <PackageTags>Roslyn Project Property Page Editors VisualStudio</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.VisualStudio.Setup.Configuration.Interop" />
  </ItemGroup>
